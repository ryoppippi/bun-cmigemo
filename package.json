{
	"name": "bun-cmigemo",
	"type": "module",
	"module": "cli.ts",
	"scripts": {
		"cli": "bun run cli.ts",
		"app": " bun run --hot ./app/index.tsx",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"prepare": "nlx bunpare && nr cmigemo:fetch && nr cmigemo:build",
		"cmigemo:fetch": "nlx giget --forceClean gh:koron/cmigemo#master",
		"cmigemo:build": "bun run ./scripts/build-cmigemo.ts",
		"bunsh": "bun run --shell bun"
	},
	"peerDependencies": {
		"typescript": "^5.6.2"
	},
	"devDependencies": {
		"@antfu/ni": "^0.23.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@types/bun": "latest",
		"cleye": "^1.3.2",
		"consola": "^3.2.3",
		"eslint": "^9.12.0",
		"eslint-plugin-format": "^0.1.2",
		"hono": "^4.6.3"
	},
	"dependencies": {
		"uint8array-extras": "^1.4.0"
	}
}
